  },
  "description": "Unprefixes vendor prefixes in legacy CSS.",
  "devDependencies": {
    "cover": "^0.2.9",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-stylefmt": "^1.1.0",
    "mocha": "^3.3.0",
    "node-version-check": "^2.1.1",
