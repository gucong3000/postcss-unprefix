    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/postcss-unprefix.git"
  },
  "scripts": {
