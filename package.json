    "postcss-less": "^0.15.0",
    "postcss-reporter": "^3.0.0",
    "stylelint": "^7.10.1"
  },
  "homepage": "https://github.com/gucong3000/postcss-unprefix#readme",
  "keywords": [
    "postcss",
    "prefix",
    "unprefix",
    "clear",
    "clean",
    "remove"
